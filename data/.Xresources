#############################################

! My Color Scheme

! black
URxvt*color0  : #2E3436
URxvt*color8  : #6C6C6C
!URxvt*color8  : #8EA5A9
!URxvt*color8  : #555753
! red
URxvt*color1  : #CC0000
URxvt*color9  : #EF2929
! green
URxvt*color2  : #4E9A06
URxvt*color10 : #8AE234
! yellow
URxvt*color3  : #C4A000
URxvt*color11 : #FCE94F
! blue
URxvt*color4  : #3465A4
URxvt*color12 : #729FCF
! magenta
URxvt*color5  : #75507B
URxvt*color13 : #AD7FA8
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #34E2E2
! white
!URxvt*color7  : #D3D7CF
URxvt*color7  : #D6D6D6
URxvt*color15 : #EEEEEC
!URxvt*color15 : #FFFFFF

!*background:            S_base3
!*foreground:            S_base00
!*fadeColor:             S_base3
!*cursorColor:           S_base01
!*pointerColorBackground:S_base1
!*pointerColorForeground:S_base01

! background color (prior to tinting) (default is white)
!URxvt*background:	#F0F0F0
urxvt*depth: 32
URxvt.background: rgba:f0f0/f0f0/f0f0/eeee

! transparency - true or false (default)
!URxvt*transparent:true
!URxvt*inheritPixmap: true

! font color (default is black)
URxvt*foreground:	#323232

#############################################

urxvt*font:           xft:Dejavu Sans Mono for Powerline:style=Book:size=16:antialias=true:hinting=true,xft:Noto Sans Devanagari
urxvt*Boldfont:       xft:Dejavu Sans Mono for Powerline:style=Bold:size=16:antialias=true:hinting=true
urxvt*Italicfont:     xft:Dejavu Sans Mono for Powerline:style=Oblique:size=16:antialias=true:hinting=true
urxvt*boldItalicfont: xft:Dejavu Sans Mono for Powerline:style=Oblique:size=16:antialias=true:hinting=true

! fix font width
URxvt*letterSpace: -1
!URxvt*lineSpace: -1

#############################################

! scrollbar - true (default) or false
URxvt*scrollBar:false

! scrollbar position - left=false (default) or right=true
URxvt*scrollBar_right:true

! scrollbar style - rxvt (default), plain, next, or xterm
URxvt*scrollstyle:plain
URxvt.secondaryScroll:  true

! transparent scrollbar
URxvt*transpscrollbar:true

#############################################
